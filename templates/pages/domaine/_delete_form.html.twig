<form method="post" action="{{ path('app_domaine_delete', {'id': domaine.id}) }}" onsubmit="return confirm('Êtes-vous sûr(e) de vouloir supprimer ce domaine ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ domaine.id) }}">
    <button class="btn btn-primary mt-4 btn-sm mb-4">Supprimer</button>
</form>
